{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node", "vitest/globals"],
    "resolveJsonModule": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "exclude": ["**/dist"]
}
